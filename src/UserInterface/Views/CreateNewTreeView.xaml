<Window x:Class="UserInterface.Views.CreateNewTreeView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UserInterface.Views"
        mc:Ignorable="d"
        Title="CreateNewTreeView" Height="200" Width="200" Background="LightGray">
    <StackPanel Margin="10">
        <TextBox x:Name="newTreeName">
        </TextBox>

        <Button x:Name="btnCreateNewTree" Margin="0, 10, 0, 0" Height="30" Content="Create New Tree">

        </Button>
    </StackPanel>
</Window>
